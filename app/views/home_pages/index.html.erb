            <%= image_tag ("/assets/logo.png"), class: "img-fluid" %>
          </a>
        </div>
        <div class="col-11 header-top-right no-padding">
          <nav id="nav-menu-container">
            <ul class="nav-menu">
              <li class="menu-active">
                <a href="#home">Home</a>
              </li>
              <li>
